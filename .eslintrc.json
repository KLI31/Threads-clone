{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:react/recommended",
        "plugin: react-hooks/recommended",
        "plugin:prettier/recommended"
        ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    
    "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
    }
}
